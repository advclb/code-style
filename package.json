{
  "name": "@advclb/foobar",
  "version": "1.0.0",
  "description": "Foobar library",
  "main": "dist/foobar.umd.js",
  "module": "lib/index.js",
  "scripts": {
    "start": "npm run doc:start",
    "build": "npm run build:babel && npm run build:webpack",
    "build:babel": "rm -rf lib && babel src --out-dir lib && cp src/*css lib",
    "build:webpack": "webpack --mode=production",
    "doc:start": "webpack-dev-server --mode=development --config=doc/webpack.config.js",
    "doc:build": "webpack --mode=production --config=doc/webpack.config.js",
    "test": "jest",
    "eslint": "eslint src/**/*.js",
    "stylelint": "stylelint src/**/*.css",
    "prettier": "prettier --write src/**"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/advclb/foobar.git"
  },
  "author": "Guo Yunhe",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/advclb/foobar/issues"
  },
  "homepage": "https://github.com/advclb/foobar#readme",
  "devDependencies": {
    "@advclb/eslint-config": "^18.0.2",
    "@advclb/stylelint-config": "^0.1.1",
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-transform-react-constant-elements": "^7.7.4",
    "@babel/plugin-transform-react-inline-elements": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/runtime": "^7.7.7",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.3.2",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "prettier-eslint": "^9.0.1",
    "prettier-stylelint": "^0.4.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "sass": "^1.24.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.1",
    "stylelint": "^12.0.1",
    "stylelint-config-prettier": "^6.0.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-scss": "^3.13.0",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "peerDependencies": {
    "react": "^16.12.0"
  }
}
